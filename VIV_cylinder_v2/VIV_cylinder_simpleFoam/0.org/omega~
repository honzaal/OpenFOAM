FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 1.028591;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1.028591;
    }

    outlet
    {
        type            zeroGradient;
    }

    valec
    {
        type            zeroGradient;
    }

    #include "include/frontBackTopBottomPatches"
}


